@Library('mylibrary')_
pipeline
{
    agent any
    stages
    {
        stage('Download_Loans')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("jenkins7")
                }
            }
        }
        stage('Building_Loans')
        {
            steps
            {
                script
                {
                    cicd.buildArtifact()
                    
                }
                
            }
         }
    }

}
